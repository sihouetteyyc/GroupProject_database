CREATE TABLE Film
(
	Fid INT CONSTRAINT S_Prim PRIMARY KEY,
	Fname VARRCHAR(50) NOT NULL,
  director VARCHAR(100) NOE NULL,
  writer VARCHAR(100) NOT NULL,
  country VARCHAR(100) NOT NULL,
  lanuage VARCHAR(100) NOT NULL,
  date VARCHAR(100) NOT NULL,
  long INT NOT NULL,
  otrname VARCHAR(100) NOT NULL,
  IMDb VARCHAR(100) NOT NULL,
  intr VARCHAR(1000) NOT NULL
)
