CREATE TABLE Actor
(
  Actor_ID INT CONSTRAINT S_Prim PRIMARY KEY,
  Actor_Name VARCHAR(100) NOT NULL,
  Actor_Sex VARCHAR(10) NOT NULL,
  Actor_Constellation VARCHAR(10),
  Actor_Birth DATA,
  Actor_BirthPlace VARCHAR(100),
  Profession VARCHAR(100),
  Other_ForeignName VARCHAR(100),
  Other_ChoineseName VARCHAR(100),
  imdb_number VARCHAR(10),
  Offical_Website VARCHAR(50),
  Actor_Intro VARCHAR(600)
  
)
